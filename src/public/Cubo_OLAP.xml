<Schema name="Esquema de Ventas CPA">
  <Cube name="Ventas" visible="true" cache="true" enabled="true">
    <Table name="fact_ventas" schema="public" alias="Hechos Ventas">
    </Table>
    <Dimension type="StandardDimension" visible="true" foreignKey="id_materia" highCardinality="false" name="Materia" description="Materias Ofertadas para la Venta">
      <Hierarchy name="Dimensi&#243;n Materia" visible="true" hasAll="true">
        <Table name="dim_materia" schema="public">
        </Table>
        <Level name="Nombre Materia" visible="true" table="dim_materia" column="nombre_materia" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never">
        </Level>
        <Level name="&#193;rea de Estudio" visible="true" table="dim_materia" column="area_estudio" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" visible="true" foreignKey="id_estudiante" highCardinality="false" name="Estudiante">
      <Hierarchy name="Dimensi&#243;n Estudiante" visible="true" hasAll="true">
        <Table name="dim_estudiante" schema="public">
        </Table>
        <Level name="Nivel Educativo" visible="true" table="dim_estudiante" column="nivel_educativo" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never">
        </Level>
        <Level name="Nombre" visible="true" table="dim_estudiante" column="nombre_completo" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" visible="true" foreignKey="id_profesor" highCardinality="false" name="Profesor">
      <Hierarchy name="Dimensi&#243;n Profesor" visible="true" hasAll="true">
        <Table name="dim_profesor" schema="public">
        </Table>
        <Level name="Nombre Profesor" visible="true" table="dim_profesor" column="nombre_profesor" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never">
        </Level>
      </Hierarchy>
    </Dimension>
    <Dimension type="StandardDimension" visible="true" foreignKey="id_tiempo" highCardinality="false" name="Tiempo">
      <Hierarchy name="Dimensi&#243;n Tiempo" visible="true" hasAll="true">
        <Table name="dim_tiempo" schema="public">
        </Table>
        <Level name="A&#241;o" visible="true" table="dim_tiempo" column="anio" type="Numeric" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
        </Level>
        <Level name="Trimestre" visible="true" table="dim_tiempo" column="trimestre" type="Numeric" uniqueMembers="true" levelType="Regular" hideMemberIf="Never">
        </Level>
        <Level name="Mes" visible="true" table="dim_tiempo" column="mes" type="Numeric" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
        </Level>
      </Hierarchy>
    </Dimension>
    <Measure name="Ventas Totales" column="total_venta" formatString="BOB #,###.00" aggregator="sum" description="Total de Ventas Realizadas por CPA" visible="true">
    </Measure>
    <Measure name="Horas Impartidas" column="duracion_horas" formatString="#,### h" aggregator="sum" visible="true">
    </Measure>
    <Measure name="Cantidad de Clases" column="id_venta" aggregator="count" visible="true">
    </Measure>
    <Measure name="Precio Promedio" column="precio_unitario" formatString="BOB #,###.00" aggregator="avg" visible="true">
    </Measure>
  </Cube>
</Schema>